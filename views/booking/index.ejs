<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head') %>
    <title>Bookings</title>
</head>
<body>
    <%- include('../partials/nav') %>
    <h1>Booking Index Page</h1>
    
    <section>
          <h3>Avaiable Bookings</h3>
        <ul>
            
            <% bookings.forEach((booking) =>{ %> 
                <li><a href="/booking/<%= booking._id%>"><%= booking.day %></a> </li>
                <li><strong><%= booking.time %></strong></li>

             <%  }) %> 
        </ul>
      
    </section>

</body>
</html>